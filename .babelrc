{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "usage",
        "corejs": "3.2.1",
        "targets": {
          "browsers": "last 1 version, not dead, chrome >= 57, firefox >= 50, ie >= 11, edge >= 14, safari >= 11, opera >= 60"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "production": {
      "plugins": ["transform-react-remove-prop-types"]
    }
  }
}
